#Service to provide interface to projection of lines onto the ground
service Duckiebot.GroundProjection

option version 0.8

import Duckiebot

object GroundProjection
    function double[] estimate_homography(Duckiebot.Image im)
    function Duckiebot.Point ground_coordinate(Duckiebot.Pixel uv, Duckiebot.Vector2D normalized_uv)
    function Duckiebot.Vector2D image_coordinate(Duckiebot.Point gp)

    wire Duckiebot.Segment{list} segments
    
end object